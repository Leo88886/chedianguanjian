page {
    height: 100%;
    width: 100%;
    background: #f4f4f4;
}

.container {
    background: #f4f4f4;
    height: auto;
    overflow: hidden;
    width: 100%;
}

.userinfo {
    display: flex;
    padding: 90rpx 32rpx 48rpx 32rpx;
    justify-content: space-between;
    background: linear-gradient(to right, #f98555, #eb4033);
    width: 750rpx;
    border-radius: 0 0 40rpx 40rpx;
}

.userinfo-avatar {
    width: 160rpx;
    height: 160rpx;
    margin-right: 50rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    font-size: 32rpx;
    color: #FFF;
}

.userinfo-tuijian {
    font-size: 30rpx;
    color: #FFF;
}

.userinfo-xs {
    font-size: 26rpx;
    color: #FFF;
    background-color: gray;
    padding: 10rpx 20rpx;
    border-radius: 40rpx;
}

.usermoney,
.menuinfo {
    background: white;
    margin: 15rpx 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-radius: 20rpx;
}

.usermoney {
    margin-top: 30rpx;
    padding: 40rpx;
}

.menuinfo {
    padding: 25rpx 50rpx;
}

.usermoney-item,
.menuinfo-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    flex-basis: 20%;
}

.usermoney-item image {
    width: 60rpx;
    height: 60rpx;
}

.menuinfo-item image {
    width: 90rpx;
    height: 90rpx;
}

.item-up {
    width: 58rpx;
    height: 58rpx;
    font-size: 32rpx;
    color: black;
    font-weight: bold;
    display: flex;
    justify-content: center;
    align-items: center;

}

.item-up image {
    width: 58rpx;
    height: 58rpx;
}

.item-down {
    font-size: 28rpx;
    color: #666666;
    white-space: nowrap;
}

.profile-info {
    width: 100%;
    height: 280rpx;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    justify-content: flex-start;
    padding: 0 30.25rpx;
    background: #333;
}

.profile-info .avatar {
    height: 148rpx;
    width: 148rpx;
    border-radius: 50%;
}

.profile-info .info {
    flex: 1;
    height: 85rpx;
    padding-left: 31.25rpx;
}

.profile-info .name {
    display: block;
    height: 45rpx;
    line-height: 45rpx;
    color: #fff;
    font-size: 37.5rpx;
    margin-bottom: 10rpx;
}

.profile-info .level {
    display: block;
    height: 30rpx;
    line-height: 30rpx;
    margin-bottom: 10rpx;
    color: #7f7f7f;
    font-size: 30rpx;
}

.user-menu {
    width: 100%;
    height: auto;
    overflow: hidden;
    background: #fff;
}

.user-menu .item {
    float: left;
    width: 33.33333%;
    height: 187.5rpx;
    border-right: 1px solid rgba(0, 0, 0, .15);
    border-bottom: 1px solid rgba(0, 0, 0, .15);
    text-align: center;
}

.user-menu .item .a {
    display: flex;
    width: 100%;
    height: 100%;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.user-menu .item.no-border {
    border-right: 0;
}

.user-menu .item.item-bottom {
    border-bottom: none;
}

.user-menu .icon {
    margin: 0 auto;
    display: block;
    height: 52.803rpx;
    width: 52.803rpx;
    margin-bottom: 16rpx;
}

.user-menu .icon.order {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -437.5rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.coupon {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -62.4997rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.gift {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -187.5rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.address {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 0 no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.security {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -500rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.kefu {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -312.5rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.help {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -250rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.feedback {
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/sprites/ucenter-sdf6a55ee56-f2c2b9c2f0.png) 0 -125rpx no-repeat;
    background-size: 52.803rpx;
}

.user-menu .icon.phone {
    background: url(https://platform-wxmall.oss-cn-beijing.aliyuncs.com/upload/20180727/15011540ebe21.png) no-repeat;
    background-size: 52.803rpx;
}


.user-menu .txt {
    display: block;
    height: 24rpx;
    width: 100%;
    font-size: 24rpx;
    color: #333;
}

.logout {
    margin-top: 50rpx;
    height: 101rpx;
    width: 100%;
    line-height: 101rpx;
    text-align: center;
    background: #fff;
    color: #333;
    font-size: 30rpx;
}

.service {
    position: static;
    background-color: transparent;
    color: transparent;
    margin: 0;
    padding: 0;
    border: none;
    text-align: left;
    line-height: normal;
    display: inline;
}

.company {
    font-size: 20rpx;
    text-align: center;
    margin-top: 50px;
}

.mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.7;
}

.charge_modalDlg {
    width: 100%;
    position: fixed;
    left: 0;
    z-index: 9999;
    background-color: #fff;
    border-radius: 36rpx;
}

.charge_modal .charge_modalDlg {
    height: 488rpx;
    top: 10%;
    padding: 65rpx 60rpx 60rpx;
}

.charge_modal .charge_modalDlg .charge_title {
    font-size: 32rpx;
    font-weight: bold;
    margin-bottom: 40rpx;
    display: block;
}

.charge_modal .charge_modalDlg .charge_input {
    display: flex;
    align-items: center;
}

.charge_modal .charge_modalDlg .charge_input text {
    font-size: 60rpx;
    font-weight: bold;
    display: block;
    margin-right: 35rpx;
}

.charge_modal .charge_modalDlg .charge_input input {
    color: orange;
    font-size: 70rpx;
    height: 70rpx;
    min-height: 70rpx;
}

.charge_modal .charge_modalDlg .charge_button {
    width: 400rpx;
    background-color: #d9d9d9;
    color: orange;
    margin: 0 auto;
    position: absolute;
    bottom: 74rpx;
    left: 175rpx;
    border-radius: 20rpx;
}

.chargeSuccess_modal .charge_modalDlg,
.chargeFail_modal .charge_modalDlg {
    height: 700rpx;
    bottom: 0;
    padding-top: 150rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.chargeSuccess_modal .charge_modalDlg image,
.chargeFail_modal .charge_modalDlg image {
    width: 94rpx;
    height: 94rpx;
    margin-bottom: 60rpx;
}

.chargeSuccess_modal .charge_modalDlg .charge_title,
.chargeFail_modal .charge_modalDlg .charge_title {
    font-size: 28rpx;
    color: #333333;
    margin-bottom: 40rpx;
}

.chargeSuccess_modal .charge_modalDlg .charge_input {
    margin-bottom: 200rpx;
    display: flex;
    align-items: center;
}

.chargeSuccess_modal .charge_modalDlg .charge_input text:first-child {
    font-size: 60rpx;
    font-weight: bold;
    margin-right: 30rpx;
}

.chargeSuccess_modal .charge_modalDlg .charge_input text:last-child {
    font-size: 80rpx;
    font-weight: bold;
}

.chargeSuccess_modal .charge_modalDlg .charge_button,
.chargeFail_modal .charge_modalDlg .charge_button {
    width: 400rpx;
    background-color: #d9d9d9;
    color: orange;
    margin: 0 auto;
    position: absolute;
    bottom: 74rpx;
    left: 175rpx;
    border-radius: 20rpx;
}

.chargeFail_modal .charge_modalDlg .charge_title {
    font-size: 32rpx;
    margin-bottom: 250rpx;
}